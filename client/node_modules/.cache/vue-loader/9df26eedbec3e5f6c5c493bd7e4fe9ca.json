{"remainingRequest":"/home/stylo/bdata/client/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/stylo/bdata/client/src/components/DataMasterTables.vue?vue&type=script&lang=js&","dependencies":[{"path":"/home/stylo/bdata/client/src/components/DataMasterTables.vue","mtime":1600809865387},{"path":"/home/stylo/bdata/client/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/stylo/bdata/client/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/home/stylo/bdata/client/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/stylo/bdata/client/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["DataMasterTables.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+JA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DataMasterTables.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <div>\n      <div>\n        <b-button v-b-toggle.sidebar-1>...</b-button>\n        <b-sidebar id=\"sidebar-1\" title=\"Tables+\" shadow>\n          <div class=\"px-3 py-2\">\n            <router-link to=\"/tech_master\">Technology Master</router-link> <br>\n            <router-link to=\"/os_tech_master\">OS Tech Master</router-link> <br>\n            <router-link to=\"/data_tech_master\">Data Tech Master</router-link> <br>\n            <router-link to=\"/app_tech_master\">App Tech Master</router-link> <br>\n          </div>\n        </b-sidebar>\n  </div>\n  <div class =\"container\">\n    <div class = \"row justify-content-md-center\">\n      <h1>Data Master</h1>\n    </div>\n    <div class = \"row\">\n      <table class=\"table table-hover\">\n        <thead>\n          <tr>\n            <th scope=\"col\">technology_id</th>\n            <th scope=\"col\">Data_technology_name</th>\n            <th scope=\"col\">Data_tech_dependant_tags</th>\n            <th scope=\"col\">Data_tech_description</th>\n            <th></th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(entry, index) in response\" :key=\"index\">\n            <td>{{ entry.technology_id }}</td>\n            <td>{{ entry.Data_technology_name }}</td>\n            <td>{{ entry.Data_tech_dependant_tags }}</td>\n            <td>{{ entry.Data_tech_description }}</td>\n            <td>\n            </td>\n            <td>\n              <div class=\"btn-group\" role=\"group\">\n                <button type=\"button\" class=\"btn btn-warning btn-sm\"\n                          v-b-modal.data-update-modal\n                          @click=\"editData(entry)\">\n                          Update\n                </button>\n                <button type=\"button\" class=\"btn btn-danger btn-sm\"\n                          @click=\"onDeleteEntry(entry)\">Delete</button>\n              </div>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n      <button type=\"button\" class=\"btn btn-success btn-sm\" v-b-modal.table-modal>Add</button>\n    </div>\n  </div>\n  <b-modal ref=\"addDataModal\"\n         id=\"table-modal\"\n         title=\"Add an entry to Data Master\"\n         hide-footer>\n    <b-form @submit=\"onSubmit\" class=\"w-100\">\n      <b-form-group id = \"form-data-fk-group\"\n                    label = \"Technology name\"\n                    label-for=\"fk-input\">\n        <b-form-select id ='fk-input'\n                       v-model=\"addForm.id\"\n                       required\n                       :options=\"tech\">\n\n        </b-form-select>\n       </b-form-group>\n      <b-form-group id=\"form-data-name-group\"\n                    label=\"Data_name:\"\n                    label-for=\"form-data-name-input\">\n          <b-form-input id=\"form-data-name-input\"\n                         type=\"text\"\n                         required\n                         maxlength=50\n                         v-model=\"addForm.name\">\n                       </b-form-input>\n      </b-form-group>\n      <b-form-group id=\"form-data-tags-group\"\n                    label=\"Data_tags:\"\n                    label-for=\"form-data-tags-input\">\n          <b-form-input id=\"form-data-tags-input\"\n                         type=\"text\"\n                         required\n                         maxlength=2000\n                         v-model=\"addForm.tags\">\n                       </b-form-input>\n      </b-form-group>\n      <b-form-group id=\"form-data-decription-group\"\n                    label=\"Data_description:\"\n                    label-for=\"form-data-description-input\">\n          <b-form-input id=\"form-data-description-input\"\n                         type=\"text\"\n                         required\n                         maxlength=500\n                         v-model=\"addForm.description\">\n                       </b-form-input>\n      </b-form-group>\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n    </b-form>\n</b-modal>\n<b-modal ref=\"editDataModal\"\n         id=\"data-update-modal\"\n         title=\"Update\"\n         hide-footer>\n  <b-form @submit=\"onSubmitUpdate\" class=\"w-100\">\n    <b-form-group id = \"form-data-fk-edit-group\"\n                  label = \"Technology name\"\n                  label-for=\"fk-edit-input\">\n      <b-form-select id ='fk-edit-input'\n                     v-model=\"editForm.technology_id\"\n                     required\n                     :options=\"tech\">\n\n      </b-form-select>\n    </b-form-group>\n  <b-form-group id=\"form-name-edit-group\"\n                label=\"Data_name:\"\n                label-for=\"form-name-edit-input\">\n      <b-form-input id=\"form-name-edit-input\"\n                    type=\"text\"\n                    v-model=\"editForm.name\"\n                    required\n                    maxlength=50\n                    placeholder=\"Enter name\">\n      </b-form-input>\n    </b-form-group>\n    <b-form-group id=\"form-data-tag-edit-group\"\n                  label=\"Data_tags:\"\n                  label-for=\"form-data-tag-edit-input\">\n        <b-form-input id=\"form-data-tag-edit-input\"\n                      type=\"text\"\n                      v-model=\"editForm.tags\"\n                      required\n                      maxlength=2000\n                      placeholder=\"Enter tags\">\n        </b-form-input>\n      </b-form-group>\n    <b-form-group id=\"form-data-description-edit-group\"\n                  label=\"Data_description:\"\n                  label-for=\"form-data-description-edit-input\">\n      <b-form-input id=\"form-tech-description-edit-input\"\n                    type=\"text\"\n                    v-model=\"editForm.description\"\n                    required\n                    maxlength=500\n                    placeholder=\"Enter description\">\n      </b-form-input>\n    </b-form-group>\n    <b-button-group>\n      <b-button type=\"submit\" variant=\"primary\">Update</b-button>\n      <b-button type=\"reset\" variant=\"danger\">Cancel</b-button>\n    </b-button-group>\n  </b-form>\n</b-modal>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      response: [],\n      tech_id: [],\n      tech_map: {},\n      addForm: {\n        name: '',\n        tags: '',\n        description: '',\n      },\n      editForm: {\n        technology_id: null,\n        old_name: '',\n        name: '',\n        tags: '',\n        description: '',\n      },\n      tech: [{ text: 'Select One', value: null }],\n    };\n  },\n  methods: {\n    getData() {\n      const path = 'http://localhost:5000/get_data_master';\n      axios.get(path)\n        .then((res) => {\n          this.response = res.data;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    addData(payload) {\n      const path = 'http://127.0.0.1:5000/add_data';\n      axios.post(path, payload)\n        .then(() => {\n          this.getData();\n        })\n        .catch((error) => {\n          console.log(error);\n          this.getData();\n        });\n    },\n    onSubmit(evt) {\n      evt.preventDefault();\n      this.$refs.addDataModal.hide();\n      const payload = {\n        technology_id: this.tech_map[this.addForm.id],\n        Data_technology_name: this.addForm.name,\n        Data_tech_dependant_tags: this.addForm.tags,\n        Data_tech_description: this.addForm.description,\n      };\n      console.log(payload);\n      this.addData(payload);\n    },\n    getTechId() {\n      const path = 'http://localhost:5000/get_tech_master';\n      axios.get(path)\n        .then((res) => {\n          this.tech_id = res.data;\n          let i;\n          for (i = 0; i < this.tech_id.length; i += 1) {\n            if (!this.tech.includes(this.tech_id[i].technology_id)) {\n              this.tech.push(this.tech_id[i].technology_name);\n              this.tech_map[this.tech_id[i].technology_name] = this.tech_id[i].technology_id;\n            }\n          }\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    editData(entry) {\n      this.editForm = entry;\n      this.editForm.old_name = entry.Data_technology_name;\n    },\n    onSubmitUpdate(evt) {\n      evt.preventDefault();\n      this.$refs.editDataModal.hide();\n      const payload = {\n        technology_id: this.tech_map[this.editForm.technology_id],\n        New_Data_technology_name: this.editForm.name,\n        Data_technology_name: this.editForm.old_name,\n        Data_tech_dependant_tags: this.editForm.tags,\n        Data_tech_description: this.editForm.description,\n      };\n      this.updateData(payload, this.editForm.name);\n    },\n    // eslint-disable-next-line\n    updateData(payload, data_name) {\n\n      const path = 'http://localhost:5000/update_data/' + data_name; // eslint-disable-line\n      axios.put(path, payload)\n        .then(() => {\n          this.getData();\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n          this.getData();\n        });\n    },\n    // eslint-disable-next-line\n    removeEntry(data_name) {\n      const path = 'http://localhost:5000/delete_data/' + data_name; // eslint-disable-line\n      axios.delete(path)\n        .then(() => {\n          this.getData();\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n          this.getData();\n        });\n    },\n    onDeleteEntry(entry) {\n      this.removeEntry(entry.Data_technology_name);\n    },\n  },\n  created() {\n    this.getData();\n    this.getTechId();\n    console.log('Succesful.');\n  },\n};\n</script>\n"]}]}